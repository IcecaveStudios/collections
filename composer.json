{
    "name": "icecave/collections",
    "description": "A set of PHP collection types loosely inspired by the .NET runtime and the C++ STL.",
    "keywords": ["collection", "container", "array", "sequence", "map", "hash", "stack", "queue"],
    "homepage": "https://github.com/IcecaveStudios/collections",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "icecave/repr": "~1"
    },
    "require-dev": {
        "eloquent/liberator": "~1",
        "eloquent/typhoon": "~0.9",
        "icecave/archer": "~0.2"
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Collections": "lib",
            "Icecave\\Collections\\TypeCheck": "lib-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "lib-typhoon",
            "validator-namespace": "Icecave\\Collections\\TypeCheck",
            "use-native-callable": false
        }
    }
}
